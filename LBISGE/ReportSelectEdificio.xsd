                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Subsistema.NombreDeSubsistema, SUM(CONVERT(numeric(10, 4), InformacionArea.Consumo)) AS ConsumoTotal
FROM            Areas INNER JOIN
                         InformacionArea ON Areas.ID_area = InformacionArea.ID_area INNER JOIN
                         Edificios ON InformacionArea.ID_edificio = Edificios.ID_edificio INNER JOIN
                         Subsistema ON InformacionArea.ID_subsistema = Subsistema.ID_subsistema
WHERE        (Edificios.ID_edificio = @e) AND (Subsistema.NombreDeSubsistema = 'Iluminación Interior' OR
                         Subsistema.NombreDeSubsistema = 'Equipo audiovisual' OR
                         Subsistema.NombreDeSubsistema = 'Aire comprimido' OR
                         Subsistema.NombreDeSubsistema = 'Aire acondicionado' OR
                         Subsistema.NombreDeSubsistema = 'Aire Acondicionado' OR
                         Subsistema.NombreDeSubsistema = 'Equipo de Cocina' OR
                         Subsistema.NombreDeSubsistema = 'Equipo de oficina' OR
                         Subsistema.NombreDeSubsistema = 'Equipo de procesos de fabricación' OR
                         Subsistema.NombreDeSubsistema = 'Equipo de medicion ' OR
                         Subsistema.NombreDeSubsistema = 'Equipo de prueba ' OR
                         Subsistema.NombreDeSubsistema = 'Equipo de refrigeración' OR
                         Subsistema.NombreDeSubsistema = 'Fuentes de Voltaje ' OR
                         Subsistema.NombreDeSubsistema = 'Horno de potencia ' OR
                         Subsistema.NombreDeSubsistema = 'Iluminación Exterior ' OR
                         Subsistema.NombreDeSubsistema = 'Motores electricos ' OR
                         Subsistema.NombreDeSubsistema = 'Miscelaneos ' OR
                         Subsistema.NombreDeSubsistema = 'Transformador de potencia  ' OR
                         Subsistema.NombreDeSubsistema = 'Ventilacion Forzada ')
GROUP BY Edificios.ID_edificio, Subsistema.NombreDeSubsistema</CommandText>
